proc ModulesHelp { } {
        puts stderr "\tSets up environment for squashfs-tools 4.5.1\n"
}

module-whatis   "sets up environment for squashfs-tools 4.5.1"

prepend-path     PATH    <insert path to squashfs-tools bin>
